{
  "name": "p5studio",
  "version": "1.0.0",
  "type": "module",
  "types": "./dist/types",
  "bin": {
    "p5": "dist/cli/index.js"
  },
  "scripts": {
    "format": "prettier --write src",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "prepush": "yarn build",

    "serve:app": "vite",
    "start:cli": "node dist/cli",

    "build:cli": "esbuild ./src/cli/index.ts --bundle --platform=node --format=esm --external:./node_modules/* --outdir=dist/cli --minify",
    "build:app": "vite build",
    "build:types": "tsc --project tsconfig.build.json",
    "build": "concurrently \"yarn build:cli\" \"yarn build:app\" \"yarn build:types\"",

    "watch:cli": "yarn build:cli --watch",
    "watch:app": "yarn build:app --watch",
    "watch:types": "yarn build:types --watch",
    "watch": "concurrently \"yarn watch:cli\" \"yarn watch:app\" \"yarn watch:types\""
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "commander": "^9.4.0",
    "esbuild": "^0.14.48",
    "express": "^4.18.1",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@mantine/core": "^5.4.1",
    "@mantine/hooks": "^5.4.1",
    "@reduxjs/toolkit": "^1.8.3",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/file-saver": "^2.0.5",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/node": "^18.0.3",
    "@types/p5": "^1.4.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^1.3.2",
    "concurrently": "^7.2.2",
    "date-fns": "^2.29.3",
    "file-saver": "^2.0.5",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "p5": "^1.4.1",
    "p5.js-svg": "^1.3.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.5.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.13"
  }
}
